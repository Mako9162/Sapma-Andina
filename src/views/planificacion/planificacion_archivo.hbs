<body class="with-side-menu">
	{{> navbar }}
	<div class="mobile-menu-left-overlay-"></div>
	{{> nav }}
	<div class="page-content">
		<div class="container-fluid">
			<header class="section-header">
				<div class="tbl">
					<div class="tbl-row">
						<div class="tbl-cell">
							<h3>Planificación desde archivo</h3>
							<ol class="breadcrumb breadcrumb-simple">
								<li><a href="/home">Inicio</a></li>
								<li class="active">Planificación desde archivo</li>
							</ol>
						</div>
                    </div>    
				</div>
			</header>
            <div class="box-typical box-typical-padding">
                <section class="card box-typical-full-height" style="min-height: 651px;">
                    <div class="card-block">
                    <div class="row">
                        <div class="col-md-3 col-sm-6">
                            <label class="form-label" >Gerencia:</label>
                            <select class="select2" name="gerencia" id="gerencia">
                                <option value="">Seleccione una gerencia</option>
                                {{#each gerencias}}
                                <option value="{{vcgas_idGerencia}}">{{vcgas_gerenciaN}}</option>
                                {{/each}}
                            </select>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <label class="form-label" >Area:</label>
                            <select class="select2" name="area" id="area">
                                <option value="">Seleccione un área</option>
                            </select>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <label class="form-label" >Sector:</label>
                                <select class="select2" name="sector" id="sector">
                                <option value="">Seleccione un sector</option>
                            </select>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <label class="form-label" >Equipo:</label>
                                <select class="select2" name="equipo" id="equipo">
                                <option value="">Seleccione un equipo</option>
                            </select>
                        </div>
                        <div class="col-md-12 col-sm-6">
                            <br>
                        </div>  
                        <div class="col-md-12 col-sm-6 text-right">
                            <button class="btn btn-sm btn-warning" id="crear_plantilla" name="crear_plantilla">Generar plantilla</button>
                        </div>   
                        <h5 class="m-t-lg with-border" style="color: aliceblue;">.</h5>
                        <div class="col-lg-6">
                            <h5 class="m-t-lg with-border">Rango de fechas</h5>
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="date1">Desde:</label>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <select class="select2" id="date1">
                                                    <option value="" selected disabled>Seleccione un mes</option>
                                                    <option value="jan">Enero</option>
                                                    <option value="feb">Febrero</option>
                                                    <option value="mar">Marzo</option>
                                                    <option value="apr">Abril</option>
                                                    <option value="may">Mayo</option>
                                                    <option value="jun">Junio</option>
                                                    <option value="jul">Julio</option>
                                                    <option value="aug">Agosto</option>
                                                    <option value="sep">Septiembre</option>
                                                    <option value="oct">Octubre</option>
                                                    <option value="nov">Noviembre</option>
                                                    <option value="dec">Diciembre</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <select class="select2" id="ano1">
                                                    <option value="" selected disabled>Seleccione un año</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                </div>
                                <div class="col-md-8">
                                    <div class="form-group">
                                        <label for="date2">Hasta:</label>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <select class="select2" id="date2">
                                                    <option value="" selected disabled>Seleccione un mes</option>
                                                    <option value="jan">Enero</option>
                                                    <option value="feb">Febrero</option>
                                                    <option value="mar">Marzo</option>
                                                    <option value="apr">Abril</option>
                                                    <option value="may">Mayo</option>
                                                    <option value="jun">Junio</option>
                                                    <option value="jul">Julio</option>
                                                    <option value="aug">Agosto</option>
                                                    <option value="sep">Septiembre</option>
                                                    <option value="oct">Octubre</option>
                                                    <option value="nov">Noviembre</option>
                                                    <option value="dec">Diciembre</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <select class="select2" id="ano2">
                                                    <option value="" selected disabled>Seleccione un año</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                </div>
                            </div>
                            <h5 class="m-t-lg with-border">Carga de archivo excel</h5>
                            <div class="row">
                                <div class="col-md-8">
                                    <input type="file" class="form-control" id="file-input" accept=".csv, application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
                                </div>
                                <div class="col-md-4">
                                    <button type="button" class="btn btn-success" id="plan_archivo" name="plan_archivo">Planificar</button>
                                </div>
                            </div>
                        </div>
                    {{!-- <div class="col-lg-6 text-right">
                        <button class="btn btn-sm btn-warning" id="descarga_excel" name="descarga_excel">Descargar plantilla</button>
                    </div> --}}
                    </div>
                </div>
                </section>
            </div>
		</div>	
	</div>
	<script src="/js/lib/jquery/jquery.min.js"></script>
	<script src="/js/lib/tether/tether.min.js"></script>
	<script src="/js/lib/bootstrap/bootstrap.min.js"></script>
	<script src="/js/plugins.js"></script>
	<script src="/js/app.js"></script>
	<script src="/js/lib/datatables-net/datatables.min.js"></script>
	<script src="/js/lib/bootstrap-sweetalert/sweetalert.min.js"></script>
	<script src="/js/lib/summernote/summernote.min.js"></script>
	<script src="/js/lib/fancybox/jquery.fancybox.pack.js"></script>
	<script src="/js/summernote-ES.js"></script>
	<script src="/js/lib/select2/select2.full.min.js"></script>
	<script src="/plan_archivo.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
</body>	