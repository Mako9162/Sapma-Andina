									isInTable = true;
									return false;
								}
							});

							// Agregar la opción solo si el protocolo no está en la tabla
							if (!isInTable) {
								$('#protocolo-correspondiente').append(`<option value="${protocolo.Id}">${protocolo.Descripcion}</option>`);
